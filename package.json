{
  "name": "aviationweather",
  "version": "1.4.0",
  "description": "Node.js wrapper for aviation weather, written in TypeScript",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:fixup",
    "build:esm": "tsc",
    "build:cjs": "tsc --outDir build/cjs",
    "build:fixup": "node --loader ts-node/esm scripts/fixup.mjs",
    "type-check": "tsc --noemit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/aviationweather.git"
  },
  "keywords": [
    "aviation",
    "flight",
    "node",
    "wrapper",
    "weather"
  ],
  "author": "oceanroleplay <indianoceanroleplay@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oceanroleplay/aviationweather/issues"
  },
  "homepage": "https://github.com/oceanroleplay/aviationweather#readme",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "prettier": "^2.4.1",
    "shelljs": "^0.8.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "fast-xml-parser": "^3.21.1"
  }
}
